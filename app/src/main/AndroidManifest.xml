<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.qihu.recyclerviewchoice"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.qihu.recyclerviewchoice.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.qihu.recyclerviewchoice.activity.MultipleChoiceHeadFootActivity"/>
        <activity android:name="com.qihu.recyclerviewchoice.activity.SingleChoiceHeadFootActivity"/>
        <activity android:name="com.qihu.recyclerviewchoice.activity.MultipleChoiceActivity"/>
        <activity android:name="com.qihu.recyclerviewchoice.activity.BgMultipleChoiceActivity"/>
        <activity android:name="com.qihu.recyclerviewchoice.activity.BgSingleChoiceActivity"/>
        <activity android:name="com.qihu.recyclerviewchoice.activity.SingleChoiceActivity"/>
    </application>

</manifest>